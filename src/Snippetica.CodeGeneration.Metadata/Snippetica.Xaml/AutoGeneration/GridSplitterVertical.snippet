<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>v GridSplitter (vertical)</Title>
    <Shortcut>GridSplitter</Shortcut>
    <Description>GridSplitter (vertical)</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-GenerateAlternativeShortcut</Keyword>
      <Keyword>Meta-NonUniqueShortcut</Keyword>
      <Keyword>Meta-TitleStartsWithShortcut</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>width</ID>
        <ToolTip>Width</ToolTip>
        <Default>5</Default>
      </Literal>
    </Declarations>
    <Code Language="Xaml"><![CDATA[<GridSplitter VerticalAlignment="Stretch" Width="$width$" />$end$]]></Code>
  </Snippet>
</CodeSnippet>