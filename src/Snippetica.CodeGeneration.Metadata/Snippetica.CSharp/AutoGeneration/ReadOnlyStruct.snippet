<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>read-only struct</Title>
    <Shortcut>rst</Shortcut>
    <Description>read-only struct declaration</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-GenerateInternalModifier</Keyword>
      <Keyword>Meta-GeneratePrivateModifier</Keyword>
      <Keyword>Meta-GeneratePublicModifier</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>_modifiers</ID>
        <ToolTip>Modifier(s)</ToolTip>
        <Default>public</Default>
      </Literal>
      <Literal>
        <ID>name</ID>
        <ToolTip>Structure name</ToolTip>
        <Default>StructName</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp"><![CDATA[$_modifiers$ readonly struct $name$ {
	$end$
}]]></Code>
  </Snippet>
</CodeSnippet>