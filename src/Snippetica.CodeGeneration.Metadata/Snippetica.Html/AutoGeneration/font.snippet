<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>font</Title>
    <Shortcut>font</Shortcut>
    <Description>font</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-NotSupportedInHtml5</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>attribute</ID>
        <Default>attribute=""</Default>
      </Literal>
      <Literal>
        <ID>content</ID>
        <Default>content</Default>
      </Literal>
    </Declarations>
    <Code Language="Html"><![CDATA[<font $attribute$>$content$</font>$end$]]></Code>
  </Snippet>
</CodeSnippet>