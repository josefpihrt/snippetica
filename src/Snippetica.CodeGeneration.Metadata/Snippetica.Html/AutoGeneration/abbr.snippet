<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>abbr</Title>
    <Shortcut>abbr</Shortcut>
    <Description>abbr</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>content</ID>
        <Default>content</Default>
      </Literal>
      <Literal>
        <ID>title</ID>
        <Default>title</Default>
      </Literal>
    </Declarations>
    <Code Language="Html"><![CDATA[<abbr title="$title$">$content$</abbr>$end$]]></Code>
  </Snippet>
</CodeSnippet>