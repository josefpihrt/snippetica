<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>blockquote</Title>
    <Shortcut>blockquote</Shortcut>
    <Description>blockquote</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>cite</ID>
        <Default>url</Default>
      </Literal>
      <Literal>
        <ID>content</ID>
        <Default>content</Default>
      </Literal>
    </Declarations>
    <Code Language="Html"><![CDATA[<blockquote cite="$cite$">$content$</blockquote>$end$]]></Code>
  </Snippet>
</CodeSnippet>