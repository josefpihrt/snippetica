<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>meter</Title>
    <Shortcut>meter</Shortcut>
    <Description>meter</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>content</ID>
        <Default>content</Default>
      </Literal>
      <Literal>
        <ID>value</ID>
        <Default>0</Default>
      </Literal>
    </Declarations>
    <Code Language="Html"><![CDATA[<meter value="$value$">$content$</meter>$end$]]></Code>
  </Snippet>
</CodeSnippet>