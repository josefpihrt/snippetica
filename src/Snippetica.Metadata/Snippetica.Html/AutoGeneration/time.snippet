<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>time</Title>
    <Shortcut>time</Shortcut>
    <Description>time</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>content</ID>
        <Default>content</Default>
      </Literal>
      <Literal>
        <ID>datetime</ID>
        <ToolTip>Date and time</ToolTip>
        <Default>datetime</Default>
      </Literal>
    </Declarations>
    <Code Language="Html"><![CDATA[<time datetime="$datetime$">$content$</time>$end$]]></Code>
  </Snippet>
</CodeSnippet>