<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>bdo</Title>
    <Shortcut>bdo</Shortcut>
    <Description>bdo</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>content</ID>
        <Default>content</Default>
      </Literal>
      <Literal>
        <ID>dir</ID>
        <ToolTip>Text direction</ToolTip>
        <Default>rtl</Default>
      </Literal>
    </Declarations>
    <Code Language="Html"><![CDATA[<bdo dir="$dir$">$content$</bdo>$end$]]></Code>
  </Snippet>
</CodeSnippet>