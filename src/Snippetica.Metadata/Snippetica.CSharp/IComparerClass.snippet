    <Code Language="CSharp"><![CDATA[$modifier$ class $name$ : IComparer {
	public int Compare(object x, object y) {
		if (object.ReferenceEquals(x, y)) {
			return 0;
		}

		if (x is null) {
			return (y is null) ? 0 : -1;
		}

		if (y is null) {
			return 1;
		}

		var a = ($type$)x;
		var b = ($type$)y;

		$end$
		throw new NotImplementedException();
	}
}]]></Code>
