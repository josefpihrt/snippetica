<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>pair with string value</Title>
    <Shortcut>ps</Shortcut>
    <Description>pair with string value</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>name</ID>
        <ToolTip>Name</ToolTip>
        <Default>name</Default>
      </Literal>
      <Literal>
        <ID>value</ID>
        <ToolTip>Value</ToolTip>
        <Default>x</Default>
      </Literal>
    </Declarations>
    <Code Language="JSON"><![CDATA["$name$": "$value$"$end$]]></Code>
  </Snippet>
</CodeSnippet>