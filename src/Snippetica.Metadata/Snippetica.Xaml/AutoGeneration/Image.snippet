<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>Image</Title>
    <Shortcut>Image</Shortcut>
    <Description>Image</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-GenerateAlternativeShortcut</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>source</ID>
        <ToolTip>Source</ToolTip>
        <Default>{Binding }</Default>
      </Literal>
    </Declarations>
    <Code Language="Xaml"><![CDATA[<Image Source="$source$" />$end$]]></Code>
  </Snippet>
</CodeSnippet>