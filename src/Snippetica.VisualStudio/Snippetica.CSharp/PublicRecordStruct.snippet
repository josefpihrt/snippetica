<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>public record struct</Title>
    <Shortcut>rdst</Shortcut>
    <Description>public record struct declaration</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-AutoGenerated</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>_parameters</ID>
        <ToolTip>List of parameters</ToolTip>
        <Default>T parameter</Default>
      </Literal>
      <Literal>
        <ID>name</ID>
        <ToolTip>Struct name</ToolTip>
        <Default>StructName</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp"><![CDATA[public record struct $name$($_parameters$);$end$]]></Code>
  </Snippet>
</CodeSnippet>