<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>Friend enum</Title>
    <Shortcut>iem</Shortcut>
    <Description>Friend enum declaration</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-AutoGenerated</Keyword>
      <Keyword>Meta-ExcludeFromReadme</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>enum</ID>
        <ToolTip>Enum name</ToolTip>
        <Default>EnumName</Default>
      </Literal>
    </Declarations>
    <Code Language="VB"><![CDATA[Friend Enum $enum$
	$end$
End Enum]]></Code>
  </Snippet>
</CodeSnippet>