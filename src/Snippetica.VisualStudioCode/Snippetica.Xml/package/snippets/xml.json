{
  "CDATA section": {
    "prefix": "cdata",
    "body": [
      "<![CDATA[${TM_SELECTED_TEXT}${0}]]>"
    ],
    "description": "CDATA section"
  },
  "CDATA section (with content)": {
    "prefix": "cdatax",
    "body": [
      "<![CDATA[${1:content}]]>${0}"
    ],
    "description": "CDATA section (with content)"
  },
  "comment": {
    "prefix": "c",
    "body": [
      "<!-- ${TM_SELECTED_TEXT}${1:comment} -->${0}"
    ],
    "description": "comment"
  },
  "element": {
    "prefix": "e",
    "body": [
      "<${1:element}>",
      "\t${TM_SELECTED_TEXT}${0}",
      "</${1}>"
    ],
    "description": "element"
  },
  "element (with attribute, with content)": {
    "prefix": "eax",
    "body": [
      "<${1:element} ${2:attribute}=\"${3:value}\">${4:content}</${1}>${0}"
    ],
    "description": "element (with attribute, with content)"
  },
  "element (with attribute)": {
    "prefix": "ea",
    "body": [
      "<${1:element} ${2:attribute}=\"${3:value}\">",
      "\t${TM_SELECTED_TEXT}${0}",
      "</${1}>"
    ],
    "description": "element (with attribute)"
  },
  "element (with content)": {
    "prefix": "ex",
    "body": [
      "<${1:element}>${2:content}</${1}>${0}"
    ],
    "description": "element (with content)"
  },
  "self-closing element": {
    "prefix": "es",
    "body": [
      "<${1:element} />${0}"
    ],
    "description": "self-closing element"
  },
  "self-closing element (with attribute)": {
    "prefix": "eas",
    "body": [
      "<${1:element} ${2:attribute}=\"${3:value}\" />${0}"
    ],
    "description": "self-closing element (with attribute)"
  }
}