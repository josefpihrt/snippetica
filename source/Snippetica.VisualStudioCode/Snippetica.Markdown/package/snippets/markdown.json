{
  "bold": {
    "prefix": "b",
    "body": [
      "**${TM_SELECTED_TEXT}${1:x}**${0}"
    ],
    "description": "bold"
  },
  "CDATA section": {
    "prefix": "cdata",
    "body": [
      "<![CDATA[${TM_SELECTED_TEXT}${0}]]>"
    ],
    "description": "CDATA section"
  },
  "CDATA section (with content)": {
    "prefix": "cdatax",
    "body": [
      "<![CDATA[${1:content}]]>${0}"
    ],
    "description": "CDATA section (with content)"
  },
  "code (inline)": {
    "prefix": "ci",
    "body": [
      "`${TM_SELECTED_TEXT}${1:x}`${0}"
    ],
    "description": "code (inline)"
  },
  "code block": {
    "prefix": "cb",
    "body": [
      "```${1:language}",
      "${TM_SELECTED_TEXT}${0}",
      "```"
    ],
    "description": "code block"
  },
  "code block (with content)": {
    "prefix": "cbx",
    "body": [
      "```${1:language}",
      "${TM_SELECTED_TEXT}${2:x}",
      "```${0}"
    ],
    "description": "code block (with content)"
  },
  "comment": {
