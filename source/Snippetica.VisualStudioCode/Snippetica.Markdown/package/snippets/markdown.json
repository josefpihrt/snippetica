  },
  "element": {
    "prefix": "e",
    "body": [
      "<${1:element}>",
      "\t${TM_SELECTED_TEXT}${0}",
      "</${1}>"
    ],
    "description": "element"
  },
  "element (with attribute)": {
    "prefix": "ea",
    "body": [
