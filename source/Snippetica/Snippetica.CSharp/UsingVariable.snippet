<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>v using variable</Title>
    <Shortcut>u</Shortcut>
    <Description>using variable</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-Tag NonUniqueShortcut</Keyword>
      <Keyword>Meta-Tag TitleStartsWithShortcut</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>_identifier</ID>
        <ToolTip>Identifier name</ToolTip>
        <Default>x</Default>
      </Literal>
      <Literal Editable="false">
        <ID>_type</ID>
        <ToolTip>Declaration type</ToolTip>
        <Default>var</Default>
      </Literal>
      <Literal>
        <ID>_value</ID>
        <ToolTip>Value to assign</ToolTip>
        <Default>expression</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp"><![CDATA[using ($_type$ $_identifier$ = $_value$)
{
    $end$
}]]></Code>
  </Snippet>
</CodeSnippet>