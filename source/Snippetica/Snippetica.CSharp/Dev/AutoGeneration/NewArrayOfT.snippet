<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>new array @ofType@</Title>
    <Shortcut>na</Shortcut>
    <Description>new array @ofType@</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-Array</Keyword>
      <Keyword>Meta-GenerateBasicType</Keyword>
      <Keyword>Meta-GenerateInitializer</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>_arrayLength</ID>
        <ToolTip>Array length</ToolTip>
        <Default>0</Default>
      </Literal>
      <Literal Editable="false">
        <ID>_initializer</ID>
        <ToolTip>Initializer</ToolTip>
      </Literal>
      <Literal>
        <ID>_type</ID>
        <ToolTip>Element type name</ToolTip>
        <Default>object</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp"><![CDATA[new $_type$[$_arrayLength$]$_initializer$$end$]]></Code>
  </Snippet>
</CodeSnippet>