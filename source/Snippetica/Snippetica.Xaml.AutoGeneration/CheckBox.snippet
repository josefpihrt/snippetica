<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>CheckBox</Title>
    <Shortcut>CheckBox</Shortcut>
    <Description>CheckBox</Description>
    <Author>Josef Pihrt</Author>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
    <Keywords>
      <Keyword>Meta-Tag GenerateAlternativeShortcut</Keyword>
    </Keywords>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>binding</ID>
        <ToolTip>Binding</ToolTip>
        <Default>Binding</Default>
      </Literal>
      <Literal>
        <ID>content</ID>
        <ToolTip>Content</ToolTip>
        <Default>Content</Default>
      </Literal>
    </Declarations>
    <Code Language="Xaml"><![CDATA[<CheckBox IsChecked="{Binding $binding$}">$content$</CheckBox>$end$]]></Code>
  </Snippet>
</CodeSnippet>